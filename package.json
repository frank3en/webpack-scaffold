    "css-loader": "6.5.1",
    "eslint": "8.5.0",
    "html-webpack-plugin": "5.5.0",
    "sass": "1.45.2",
    "sass-loader": "12.4.0",
    "style-loader": "3.3.1",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.2",
    "webpack-merge": "5.8.0"
  }
}
